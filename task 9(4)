#include <iostream>
#include <vector>

int main() {
    std::vector<int> arr = {3, 8, 1, 7, 5, 10, 2}; // заданий масив
    int A;

    // Введення числа A з клавіатури
    std::cout << "Введіть число A: ";
    std::cin >> A;

    // Підрахунок елементів, більших за A
    int count = 0;
    for (int i = 0; i < arr.size(); i++) {
        if (arr[i] > A) {
            count++;
        }
    }

    // Виведення результату
    std::cout << "Кількість елементів, більших за " << A << ": " << count << std::endl;

    return 0;
}
